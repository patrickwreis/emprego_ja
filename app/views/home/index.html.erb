

<div class="row">
  <div class="col-md-9">
    <section class="jobs">
      <div class="row">
        <% @jobs.each do |job| %>
        <div class="col-md-6">
          <%= render job %>
        </div>
        <% end %>
      </div>
    </section>
  </div>

  <div class="col-md-3">
    <aside class="companies">
      <div>
        <h1>Empresas</h1>
        <div class="list-group">
            <% @companies.each do |company| %>
              <div class="list-group-item"><%= link_to company.name, company%></div>
            <% end %>
        </div>
      </div>
    </aside>
  </div>

</div>
