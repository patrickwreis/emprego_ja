

<%= link_to 'Criar Vaga', new_job_path%> |
<%= link_to 'Criar Empresa', new_company_path%>

<div class="row">
  <div class="col-md-9">
    <section class="jobs">
      <div class="row">
        <% @jobs.each do |job| %>
        <div class="col-md-6">
          <%= render job %>
        </div>
        <% end %>
      </div>
    </section>
  </div>

  <div class="col-md-3">
    <aside class="companies">
      <div>
        <h1>Empresas</h1>
        <ul class="list-unstyled">
          <% @companies.each do |company| %>
          <li>
            <%= link_to company.name, company%>
          </li>
          <% end %>
        </ul>
      </div>
    </aside>
  </div>

</div>
